{
  "dashboard": {
    "title": "QNWIS Ops Console Metrics",
    "tags": ["qnwis", "ops", "alerts", "incidents"],
    "timezone": "utc",
    "editable": true,
    "graphTooltip": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Notifications Sent (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(qnwis_notifications_sent_total[5m])",
            "legendFormat": "{{channel}} - {{severity}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Notifications/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Notification Failures",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(qnwis_notifications_failed_total[5m])",
            "legendFormat": "{{channel}} - {{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Failures/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Notification Failure Rate"
        }
      },
      {
        "id": 3,
        "title": "Notification Retries",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(qnwis_notifications_retried_total[5m])",
            "legendFormat": "{{channel}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Retries/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Incident Lifecycle Counts",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "qnwis_incidents_total",
            "legendFormat": "{{state}}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none"
        }
      },
      {
        "id": 5,
        "title": "Notification Dispatch Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(qnwis_notification_dispatch_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qnwis_notification_dispatch_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qnwis_notification_dispatch_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 50, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 6,
        "title": "Notification Routing Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(qnwis_notification_route_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qnwis_notification_route_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qnwis_notification_route_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 60, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 7,
        "title": "Ops Console Page Render Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(qnwis_ops_console_render_duration_ms_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qnwis_ops_console_render_duration_ms_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qnwis_ops_console_render_duration_ms_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "ms", "label": "Render Time"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 150, "colorMode": "critical", "op": "gt", "fill": true, "line": true}
        ]
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "rate(qnwis_cache_hits_total[5m]) / (rate(qnwis_cache_hits_total[5m]) + rate(qnwis_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "SSE Connections Active",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "qnwis_sse_connections_active",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "CSRF Token Validation Failures",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 32},
        "targets": [
          {
            "expr": "rate(qnwis_csrf_validation_failures_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Ops Console Requests by Endpoint",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "rate(qnwis_ops_console_requests_total[5m])",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {"endpoint": 0, "Value": 1}
            }
          }
        ]
      },
      {
        "id": 12,
        "title": "Alert Evaluations by Rule",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "rate(qnwis_alert_evaluations_total[5m])",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {"rule_id": 0, "triggered": 1, "Value": 2}
            }
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "interval",
          "type": "interval",
          "query": "30s,1m,5m,15m,30m,1h",
          "current": {
            "text": "5m",
            "value": "5m"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(qnwis_build_info[5m]) > 0",
          "titleFormat": "Deployment",
          "textFormat": "Version {{ version }}"
        }
      ]
    }
  },
  "overwrite": true
}
